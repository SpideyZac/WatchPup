{
    "name": "watchpup-api",
    "version": "1.0.0",
    "description": "API for WatchPup",
    "main": "./src/app.ts",
    "scripts": {
        "build": "bunx tsc && resolve-tspaths",
        "start": "ENV_TYPE=production bun ./dist/app.js",
        "dev": "bun --hot ./src/app.ts",
        "clean:build": "rm -rf dist",
        "clean:all": "rm -rf dist && rm -rf node_modules",
        "format": "prettier --write .",
        "lint": "npx tsc --noEmit && eslint .",
        "lint:fix": "npx tsc --noEmit && eslint --fix ."
    },
    "type": "module",
    "keywords": [
        "watchpup",
        "api"
    ],
    "author": "SpideyZac",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@types/bun": "latest",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.21",
        "@typescript-eslint/eslint-plugin": "^8.3.0",
        "@typescript-eslint/parser": "^8.3.0",
        "eslint": "^9.9.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.0.0",
        "resolve-tspaths": "^0.8.19",
        "typescript-eslint": "^8.3.0"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "cookie-parser": "^1.4.6",
        "express": "^4.19.2",
        "surrealdb.js": "^1.0.0-beta.9",
        "zod": "^3.23.8"
    }
}
