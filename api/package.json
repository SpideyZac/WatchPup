{
    "name": "watchpup-api",
    "version": "1.0.0",
    "description": "API for WatchPup",
    "main": "./src/app.ts",
    "scripts": {
        "build": "bunx tsc && resolve-tspaths",
        "start": "ENV_TYPE=production bun ./dist/app.js",
        "dev": "bun --hot ./src/app.ts",
        "clean:build": "rm -rf dist",
        "clean:all": "rm -rf dist && rm -rf node_modules",
        "format": "prettier --write .",
        "lint": "npx tsc --noEmit && eslint .",
        "lint:fix": "npx tsc --noEmit && eslint --fix ."
    },
    "type": "module",
    "keywords": [
        "watchpup",
        "api"
    ],
    "author": "SpideyZac",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@types/bun": "latest",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^8.12.1",
        "@typescript-eslint/parser": "^8.12.1",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "nodemon": "^3.1.7",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "resolve-tspaths": "^0.8.22",
        "typescript-eslint": "^8.12.1"
    },
    "peerDependencies": {
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "cookie-parser": "^1.4.7",
        "express": "^4.21.1",
        "express-rate-limit": "^7.4.1",
        "jsonwebtoken": "^9.0.2",
        "surrealdb": "^1.0.6",
        "zod": "^3.23.8"
    }
}
